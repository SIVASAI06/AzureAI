{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Adf-gen2"
		},
		"ADLSGen2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ADLSGen2'"
		},
		"AzureDBLS_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureDBLS'"
		},
		"AzureDLSLS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDLSLS'"
		},
		"AzureDBLS_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "adfgen2.database.windows.net"
		},
		"AzureDBLS_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "taxi-data"
		},
		"AzureDBLS_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sivasai"
		},
		"AzureDLSLS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dbtogen2.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "Adf-gen2_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLSGen2_connectionString": {
						"value": "[parameters('ADLSGen2_connectionString')]"
					},
					"AzureDBLS_password": {
						"value": "[parameters('AzureDBLS_password')]"
					},
					"AzureDLSLS_accountKey": {
						"value": "[parameters('AzureDLSLS_accountKey')]"
					},
					"AzureDBLS_properties_typeProperties_server": {
						"value": "[parameters('AzureDBLS_properties_typeProperties_server')]"
					},
					"AzureDBLS_properties_typeProperties_database": {
						"value": "[parameters('AzureDBLS_properties_typeProperties_database')]"
					},
					"AzureDBLS_properties_typeProperties_userName": {
						"value": "[parameters('AzureDBLS_properties_typeProperties_userName')]"
					},
					"AzureDLSLS_properties_typeProperties_url": {
						"value": "[parameters('AzureDLSLS_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}